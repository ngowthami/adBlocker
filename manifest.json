{
    "name": "urlBlocker",
    "version": "1.0",
    "description": "Chrome URLs Blocking Extension",
    "permissions": ["webRequest", "webRequestBlocking", "<all_urls>","tabs","http://*/", "https://*/"],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "Ad-blocker.png"
    },
    "manifest_version": 2,
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+B",
                "mac": "MacCtrl+Shift+B"
            },
            "description": "Activates urlBlocker"
        }
    }
}
  